{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "CopyBlobToSynapseDemo",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						}
					},
					"enableStaging": true,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Year",
									"type": "Int64"
								},
								"sink": {
									"name": "Year",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Passenger Fatalities",
									"type": "Int64"
								},
								"sink": {
									"name": "PassengerFatalities",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Passenger Serious Injuries",
									"type": "Int64"
								},
								"sink": {
									"name": "PassengerSeriousInjuries",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Total Passenger Enplanements (millions)",
									"type": "Int64"
								},
								"sink": {
									"name": "TotalPassengerEnplanements(millions)",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Million Passenger Enplanements per Passenger Fatality",
									"type": "Decimal"
								},
								"sink": {
									"name": "MillionPassengerEnplanementsPerPassengerFatality",
									"type": "Decimal"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_Src_blob_adfdemoblob",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_Synapse_Demo",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}