{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-eus-demo"
		},
		"Dest_SQL_adfdemo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Dest_SQL_adfdemo'"
		},
		"LS_Src_Blob_eus_demo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Src_Blob_eus_demo'"
		},
		"Src_VMFile_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Src_VMFile'"
		},
		"Src_VMFile_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "F:\\data\\DemoFile"
		},
		"Src_VMFile_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "pkmishra"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Dest_SQL_adfdemo_connectionString": {
						"value": "[parameters('Dest_SQL_adfdemo_connectionString')]"
					},
					"LS_Src_Blob_eus_demo_connectionString": {
						"value": "[parameters('LS_Src_Blob_eus_demo_connectionString')]"
					},
					"Src_VMFile_password": {
						"value": "[parameters('Src_VMFile_password')]"
					},
					"Src_VMFile_properties_typeProperties_host": {
						"value": "[parameters('Src_VMFile_properties_typeProperties_host')]"
					},
					"Src_VMFile_properties_typeProperties_userId": {
						"value": "[parameters('Src_VMFile_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}